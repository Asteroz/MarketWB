                        </div>
                        <div class="tab-pane fade" id="pills-profile" role="tabpanel" aria-labelledby="pills-profile-tab" tabindex="0">
                            <canvas id="line-chart2" width="600" height="255"></canvas>
                        </div>
                      
                    </div>
                    <script>
                        var ctx = document.getElementById("line-chart").getContext("2d");
                        ctx.height = 255;
                        Chart.defaults.font.family = 'Manrope';
                        Chart.defaults.font.style = 'normal';
                        Chart.defaults.color = "#989898";
                        Chart.defaults.font.size = 11;

                        var gradient = ctx.createLinearGradient(0, 0, 600, 0);
                        gradient.addColorStop(1, 'rgba(253, 152, 107, 1)');
                        gradient.addColorStop(0.8, 'rgba(215, 49, 97, 1)');
                        gradient.addColorStop(0.7, 'rgba(202, 53, 122, 1)');
                        gradient.addColorStop(0.6, 'rgba(177, 42, 142, 1)');
                        gradient.addColorStop(0.4, 'rgba(110, 17, 161, 1)');
                        gradient.addColorStop(0.2, 'rgba(48, 4, 191, 1)');
                        gradient.addColorStop(0, 'rgba(62, 51, 187, 1)');

                        new Chart(document.getElementById("line-chart"), {
                        type: 'line',
                        data: {
                            labels: [@Html.Raw(string.Join(",",Model.DashboardReport.SalesPriceChartData.Select(o => o.Date.ToString("dd/MM/yyyy").Insert(0,"'").Insert(11,"'"))))],
                            datasets: [{ 
                                data: [@string.Join(",",Model.DashboardReport.SalesPriceChartData.Select(o => o.Value.ToString()))],
                                label: "Заказы",
                                borderColor: gradient,
                                pointBackgroundColor : "#2F2B5B",
                                pointBorderColor : "#FFF",
                                backgroundColor: 'rgba(150, 124, 253, 0.06)',
                                fill: true,
                                tension: 0.4
                            },
                            ]
                        },
                        options: {
                            plugins: {
                                legend: {
                                    display: false
                                },
                                tooltip: {
                                    backgroundColor: "#2B2F42",
                                    displayColors: false,
                                    position: 'average',
                                    titleAlign:  'center',
                                    bodyColor: "rgba(255, 255, 255, 0.72)",
                                    bodyAlign: 'center',
                                    cornerRadius: 11,
                                    padding: 15,
                                    yAlign: 'bottom',
                                    xAlign: 'center',
                                }
                            },
                            scales:{
                                x: {
                                    display: true,
                                    grid: {
                                        color: "#F6F6FB",
                                    },
                                },
                                y: {
                                    display: true,
                                    grid: {
                                        color: "#F6F6FB",
                                        display: false,
                                    },
                                },
                            },
                        }
                        });

