                        gradient.addColorStop(0.4, 'rgba(110, 17, 161, 1)');
                        gradient.addColorStop(0.2, 'rgba(48, 4, 191, 1)');
                        gradient.addColorStop(0, 'rgba(62, 51, 187, 1)');

                        new Chart(document.getElementById("line-chart2"), {
                        type: 'line',
                        data: { //хуй
                            labels: [@Html.Raw(string.Join(",",Model.DashboardReport.SalesCountChartData.Select(o => o.Date.ToString("dd/MM/yyyy").Insert(0,"'").Insert(11,"'"))))],
                            datasets: [{ 
                                data: [@Html.Raw(string.Join(",",Model.DashboardReport.SalesCountChartData.Select(o => o.Value.ToString())))],
                                label: "Заказы",
                                borderColor: gradient,
                                pointBackgroundColor : "#2F2B5B",
                                pointBorderColor : "#FFF",
                                backgroundColor: 'rgba(150, 124, 253, 0.06)',
                                fill: true,
                                tension: 0.4
                            },
                            ]
                        },
                        options: {
                            plugins: {
                                legend: {
                                    display: false
                                },
                                tooltip: {
                                    backgroundColor: "#2B2F42",
                                    displayColors: false,
                                    position: 'average',
                                    titleAlign:  'center',
                                    bodyColor: "rgba(255, 255, 255, 0.72)",
                                    bodyAlign: 'center',
                                    cornerRadius: 11,
                                    padding: 15,
                                    yAlign: 'bottom',
                                    xAlign: 'center',
                                }
                            },
                            scales:{
                                x: {
                                    display: true,
                                    grid: {
                                        color: "#F6F6FB",
                                    },
                                },
                                y: {
                                    display: true,
                                    grid: {
                                        color: "#F6F6FB",
                                        display: false,
                                    },
                                },
                            },
                        }
                        });
                    </script>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="panelwb panelwbgradient">
                            <div class="panelwb-head">
                                <div class="head-panel">
                                    <div class="name2">Прибыль</div>
